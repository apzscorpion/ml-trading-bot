# Database Configuration
DATABASE_URL=postgresql://user:password@host:5432/dbname
# For local development: sqlite:///./trading_predictions.db

# Redis Configuration (Railway provides REDIS_URL automatically)
REDIS_URL=redis://default:password@host:6379
REDIS_ENABLED=true
REDIS_TTL_SECONDS=300

# API Keys (Optional - for enhanced features)
FREDDY_API_KEY=your_freddy_api_key_here
FREDDY_ORGANIZATION_ID=your_org_id_here
FREDDY_ASSISTANT_ID=your_assistant_id_here
FREDDY_ENABLED=false

TWELVEDATA_API_KEY=your_twelvedata_api_key_here
TWELVEDATA_ENABLED=false

# CORS Settings (Update with your Railway/Vercel URLs)
ALLOWED_ORIGINS=http://localhost:5155,https://your-app.railway.app,https://your-app.vercel.app

# Application Settings
LOG_LEVEL=INFO
DEFAULT_SYMBOL=TCS.NS
PREDICTION_INTERVAL=300

# Market Settings
NIFTY_SYMBOL=^NSEI
SENSEX_SYMBOL=^BSESN

# Feature Flags
TREND_PREDICTION_ENABLED=true
MARKET_PREDICTION_ENABLED=true

# Data Provider
PRIMARY_DATA_PROVIDER=yahoo
USE_TWELVEDATA_AS_FALLBACK=false

# Model Settings
DEFAULT_HORIZON_MINUTES=180
MIN_CANDLES_FOR_PREDICTION=50

